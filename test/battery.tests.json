{
	"pattern": "test.volt",
	"testCommandPrefix": "//T ",
	"macros": {
		"default": [
			"//T run:volta -o %t %s",
			"//T run:%t"
		],
		"expect-failure": [
			"//T retval:1",
			"//T run:volta -o %t %s"
		],
		"import": [
			"//T run:volta -o %t %s --src-I %S/../deps",
			"//T run:%t"
		],
		"passpath": [
			"//T run:volta -o %t %s",
			"//T run:%t %S"
		],
		"empty": [
		]
	}
}

